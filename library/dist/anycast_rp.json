{
    "definitions": {
        "numericString": {
            "pattern": "[1-9][0-9]*", 
            "type": "string"
        }, 
        "peerCountString": {
            "pattern": "^([^\\|]+\\|){2}peer_count$", 
            "type": "string"
        }, 
        "peerIpString": {
            "pattern": "\\|((0|([1-9][0-9]?)|(1[0-9]{2})|(2([0-4][0-9]))|(25[0-5]))\\.){3}(0|([1-9][0-9]?)|(1[0-9]{2})|(2([0-4][0-9]))|(25[0-5]))\\|", 
            "type": "string"
        }, 
        "peerString": {
            "pattern": "^([^\\|]+\\|){2}peer\\|[^\\|]", 
            "type": "string"
        }, 
        "rpAddressString": {
            "pattern": "\\|((0|([1-9][0-9]?)|(1[0-9]{2})|(2([0-4][0-9]))|(25[0-5]))\\.){3}(0|([1-9][0-9]?)|(1[0-9]{2})|(2([0-4][0-9]))|(25[0-5]))\\|", 
            "type": "string"
        }, 
        "rpRoleString": {
            "pattern": "\\|rp_role$", 
            "type": "string"
        }, 
        "vrfString": {
            "pattern": "^[^\\|]+\\|", 
            "type": "string"
        }
    }, 
    "properties": {
        "identity": {
            "oneOf": [
                {
                    "allOf": [
                        {
                            "$ref": "#/definitions/vrfString"
                        }, 
                        {
                            "$ref": "#/definitions/rpAddressString"
                        }, 
                        {
                            "$ref": "#/definitions/peerString"
                        }, 
                        {
                            "$ref": "#/definitions/peerIpString"
                        }
                    ]
                }, 
                {
                    "allOf": [
                        {
                            "$ref": "#/definitions/vrfString"
                        }, 
                        {
                            "$ref": "#/definitions/rpAddressString"
                        }, 
                        {
                            "$ref": "#/definitions/peerCountString"
                        }
                    ]
                }, 
                {
                    "allOf": [
                        {
                            "$ref": "#/definitions/vrfString"
                        }, 
                        {
                            "$ref": "#/definitions/rpRoleString"
                        }
                    ]
                }
            ], 
            "type": "string"
        }, 
        "value": {
            "$ref": "#/definitions/numericString"
        }
    }, 
    "required": [
        "identity", 
        "value"
    ], 
    "type": "object"
}