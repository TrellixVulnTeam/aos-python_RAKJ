{
    "definitions": {
        "vlanString": {
            "pattern": "^vlan=[1-9][0-9]*\\|.+$"
        }, 
        "tableSize": {
            "description": "Size of VxLAN address table",
            "pattern": "[^\\|]+\\|table_size$"
        }, 
        "countOfMacsWithMultipleMoves": {
            "description": "Count of MAC addresses with multiple moves in a given VLAN",
            "pattern": "[^\\|]+\\|count_of_macs_with_multiple_moves$"
        }, 
        "macsWithMultipleMoves": {
            "description": "List of <mac|count> entries for MACs with multiple moves, encoded in Python list format",
            "pattern": "[^\\|]+\\|macs_with_multiple_moves$"
        }, 
        "macMoves": {
            "description": "MAC Address. Must be lower-case with colon separator. e.g.: f8:d7:56:00:03:13",
            "pattern": "[^\\|]+\\|([a-f0-9]{2}:){5}[a-f0-9]{2}\\|moves$"
        }
    }, 
    "properties": {
        "identity": {
            "allOf": [
                {
                    "$ref": "#/definitions/vlanString"
                }, 
                {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/tableSize"
                        }, 
                        {
                            "$ref": "#/definitions/countOfMacsWithMultipleMoves"
                        }, 
                        {
                            "$ref": "#/definitions/macsWithMultipleMoves"
                        }, 
                        {
                            "$ref": "#/definitions/macMoves"
                        }
                    ]
                }
            ],
            "type": "string"
        }, 
        "value": {
            "type": "string"
        }
    }, 
    "required": [
        "identity", 
        "value"
    ], 
    "type": "object"
}